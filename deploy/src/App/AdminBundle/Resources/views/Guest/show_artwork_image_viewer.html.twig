<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-body">

                    {% set image_url = s3_url_generator.generateUrl(object.getArtworkImagePath, true)%}
                    {% if image_url %}

                        <a href="{{ image_url }}" target="_blank">
                            <img src="{{ image_url }}" class="img-responsive" />
                        </a>

                    {% else %}
                        The artwork image has not been created yet.
                    {%  endif %}

                </div>
            </div>
        </div>
    </div>
</div>
